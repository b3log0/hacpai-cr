{
  "name": "hacpai-cr",
  "displayName": "hacpai-cr",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.hacpaicr.signin",
    "onCommand:extension.hacpaicr.signout",
    "onCommand:extension.hacpaicr.connect",
    "onCommand:extension.hacpaicr.send",
    "onView:view.hacpai.cr",
    "onView:view.hacpai.cru"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hacpaicr",
          "title": "hacpaicr",
          "icon": "resources/hacpai.svg"
        }
      ]
    },
    "views": {
      "hacpaicr": [
        {
          "id": "view.hacpai.cr",
          "name": "ChatRoom"
        },
        {
          "id": "view.hacpai.cru",
          "name": "Users"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.hacpaicr.signin",
        "title": "Hacpaicr: Signin"
      },
      {
        "command": "extension.hacpaicr.signout",
        "title": "Hacpaicr: Signout"
      },
      {
        "command": "extension.hacpaicr.connect",
        "title": "Hacpaicr: Connect"
      },
      {
        "command": "extension.hacpaicr.send",
        "title": "Hacpaicr: Send"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/websocket": "^0.0.40",
    "axios": "^0.19.0",
    "websocket": "^1.0.29"
  }
}
