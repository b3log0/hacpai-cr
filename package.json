{
  "name": "hacpi-cr",
  "displayName": "hacpi-cr",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.hacpicr.welcom",
    "onCommand:extension.hacpicr.login",
    "onCommand:extension.hacpicr.logout",
    "onView:view.hacpi.cr"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hacpicr",
          "title": "hacpicr",
          "icon": "resources/hacpi.svg"
        }
      ]
    },
    "views": {
      "hacpicr": [
        {
          "id": "view.hacpi.cr",
          "name": "hacpi Chat Room"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.hacpicr.welcom",
        "title": "Hacpicr: Welcom"
      },
      {
        "command": "extension.hacpicr.login",
        "title": "Hacpicr: Login"
      },
      {
        "command": "extension.hacpicr.logout",
        "title": "Hacpicr: Logout"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/rx": "^4.1.1",
    "rxjs": "^6.5.2"
  }
}
